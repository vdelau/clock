{
    "name": "vdelau/clock",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "vdelau",
        "clock"
    ],
    "homepage": "https://github.com/vdelau/clock",
    "license": "MIT",
    "authors": [
        {
            "name": "Vicnent de Lau",
            "email": "vincent@delau.nl",
            "homepage": "https://github.com/vdelau",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "VdeLau\\Clock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VdeLau\\Clock\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/php-fig/clock"
        }
    ],
    "require": {
        "php": "~8.0",
        "psr/clock": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
